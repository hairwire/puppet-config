[program:BillingServer]
command=/usr/java/default/bin/java -cp /usr/adagio/Adagio_Lib-<%= adagio_version %>-jar-with-dependencies.jar -Dadminserver.serverid=<%= serverid %> -Dadminserver.configfile=/usr/adagio/jndi.properties adagio.server.billing.BillingAdminServer
;process_name=%(program_name)s ; process_name expr (default %(program_name)s)
;numprocs=1;          ; number of processes copies to start (def 1)
;directory=/usr/indesign          ; directory to cwd to before exec (def no cwd)
;umask=022                     ; umask for process (default None)
priority=400                   ; the relative start priority (default 999)
;autostart=true                ; start at supervisord start (default: true)
;autorestart=true              ; retstart at unexpected quit (default: true)
startsecs=30                   ; number of secs prog must stay running (def. 1)
;startretries=3                ; max # of serial start failures (default 3)
;exitcodes=0,2                 ; 'expected' exit codes for process (default 0,2)
;stopsignal=INT                 ; signal used to kill process (default TERM)
;stopwaitsecs=120               ; max num secs to wait b4 SIGKILL (default 10)
user=nobody                     ; setuid to this UNIX account to run the program
redirect_stderr=true          ; redirect proc stderr to stdout (default false)
;environment=DISPLAY=':0'
